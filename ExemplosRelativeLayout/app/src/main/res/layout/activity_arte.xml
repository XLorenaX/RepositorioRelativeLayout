<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.example.manoel.exemplosrelativelayout.ArteActivity"
    android:background="#f5dddd">

    <ImageView
        android:layout_width="300px"
        android:layout_height="350px"
        android:id="@+id/img1"
        android:src="@drawable/img"
        android:layout_alignParentTop="true"
        android:layout_alignParentStart="true" />

    <ImageView
        android:layout_width="300px"
        android:layout_height="450px"
        android:id="@+id/img3"
        android:src="@drawable/img3"
        android:layout_alignBottom="@+id/img1"
        android:layout_alignParentEnd="true"
        android:layout_alignParentTop="@+id/img1"/>

    <ImageView
        android:layout_width="300px"
        android:layout_height="450px"
        android:id="@+id/img2"
        android:src="@drawable/img2"
        android:layout_alignParentBottom="true"
        android:layout_alignParentStart="true"
        android:layout_marginBottom="90dp"
        style="@android:style/Theme"
        android:adjustViewBounds="true" />

    <ImageView
        android:layout_width="300px"
        android:layout_height="450px"
        android:id="@+id/imageView2"
        android:src="@drawable/img9"
        android:layout_alignTop="@+id/img2"
        android:layout_alignStart="@+id/img3" />


</RelativeLayout>
